{
  "name": "panoplia.peer",
  "version": "0.1.0",
  "description": "React library for building onramping and offramping using Peer (ZKP2P) in your React wallet app",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "viem": "^2.37.3"
  },
  "dependencies": {
    "@zkp2p/sdk": "^0.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^28.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.2.4",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "viem": "^2.37.3",
    "vitest": "^4.0.18"
  },
  "license": "MIT"
}
