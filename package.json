{
  "name": "panoplia.peer",
  "version": "0.1.0",
  "description": "React library for building onramping and offramping using Peer (ZKP2P) in your React wallet app",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "viem": "^2.37.3"
  },
  "dependencies": {
    "@zkp2p/sdk": "^0.0.11"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "viem": "^2.37.3"
  },
  "license": "MIT"
}
